 #include <iostream>

using namespace std;

int main()
{ 
    int m1[2][3]={{12,11, 10}, {55,15,1}};
    int m2[2][3]={{22,33, 15},{77, 33, 10}};
    int sum[2][3];
    {
        for(int i=0; i<=1; i++)
        {
            for(int j=0; j<=2; j++)
            {
                
            sum[i][j]=m1[i][j]+m2[i][j];
             cout<<sum[i][j]<<"\t";
            }
            cout<<"\n";
        }
    }
    return 0;
}


