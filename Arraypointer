#include<iostream>
using namespace std;
int main()
{
    int *p;  
    int arr[10];
    cout<<"Enter the elements of array"<<"\n";
    for(int i=0; i<=10; i++)
    {
        cin>>arr[i];
    }
    p=arr;
    cout<<"The elements are "<<"\n";
    for(int j=0; j<=10; j++)
    {
    cout<<*(p+j)<<"\n";
    }
return 0;  
